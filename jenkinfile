pipeline {
    agent any
    tools {
        maven 'MAVEN_HOME'  // Ensure this matches your Maven tool name configured in Jenkins
    }
    stages {
        stage('Clean') {
            steps {
                bat 'mvn clean'
            }
        }
        stage('Install') {
            steps {
                bat 'mvn install'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn test'
            }
        }
        stage('Package') {
            steps {
                bat 'mvn package'
            }
        }
    }
}
